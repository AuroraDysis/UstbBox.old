<common:BootStrapper x:Class="UstbBox.App.App"
                     xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                     xmlns:common="using:Template10.Common"
                     xmlns:converters="using:UstbBox.App.Converters">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles\Custom.xaml" />
                <ResourceDictionary Source="Styles\ContentDialog.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <converters:TeachNewsItemOrderConverter x:Key="TeachNewsItemOrder"/>
        </ResourceDictionary>
    </Application.Resources>
</common:BootStrapper>
